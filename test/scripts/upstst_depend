#!/bin/sh
# ============================================================================
# test ups depend
# ============================================================================
cd $UPS_DIR/test/scripts/depend/log
$UPS_DIR/test/ups_test << EOD 

depend bee1 v1_0 -f NULL -z ../db1 -K DECLARER -out bee1.out -diff bee1.in

depend bee1 v1_0 -f NULL -z ../db1 -K ACTION -status UPS_INVALID_KEYWORD
 
depend bee1     -f NULL  -z ../db1 -m bee2.table -out bee2.out -diff bee2.in

depend bee1      -f NULL -z ../db1 -M ../db1/bee3/v2_0.table -status UPS_NO_PRODUCT_FOUND



quit
EOD
