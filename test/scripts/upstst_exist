#!/bin/sh
# ============================================================================
# test ups exist
# ============================================================================
cd $UPS_DIR/test/scripts/exist
$UPS_DIR/bin/ups exist exists v1_0 -f NULL -z ./db1
if [ ! $? ]; then echo "Exist -- failed to report existing product"; fi
$UPS_DIR/bin/ups exist doesntexist v1_0 -f NULL -z ./db1
if [ $? ]; then echo "Exist -- Incorrectly reported nonexistent product"; fi
