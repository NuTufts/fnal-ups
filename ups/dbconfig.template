FILE = DBCONFIG

# all nodes can read/use the products declared in this db
AUTHORIZED_NODES = *

# all product roots are under /usr/products:
PROD_DIR_PREFIX = /usr/products

# keep statistics about the following products (uncomment this to get it):
#STATISTICS = edt,tpu,tkreq

# manpages, info files, etc. go here:
MAN_SOURCE_DIR = /usr/products/man
CATMAN_SOURCE_DIR = /usr/products/catman
INFO_SOURCE_DIR = /usr/products/Info
HTML_SOURCE_DIR = /usr/products/SunOS/html/www-oss/docs
NEWS_SOURCE_DIR = /dev/null

# this db contains the ups product; copy the setups.[c]sh
#  and cached files to this directory
SETUPS_DIR = /usr/local/etc

# this db contains the upd product; look for the upd
#  user callouts here:
UPD_USERCODE_DIR = ../.updfiles
                
